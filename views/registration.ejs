<%- include("partials/header"); -%>

<link rel="apple-touch-icon" href="/docs/5.0/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
<link rel="icon" href="/docs/5.0/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
<link rel="icon" href="/docs/5.0/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
<link rel="manifest" href="/docs/5.0/assets/img/favicons/manifest.json">
<link rel="mask-icon" href="/docs/5.0/assets/img/favicons/safari-pinned-tab.svg" color="#7952b3">
<link rel="icon" href="/docs/5.0/assets/img/favicons/favicon.ico">
<meta name="theme-color" content="#7952b3">



<div style="background-color: #F7F4E9;"class="row-reg full-form">
  <div class="py-5 text-center">
    <h2>Registration form</h2>
    <p style="color: #ff4655;;"class="lead">Remember, there's no going back</p>
    <p style="color: #042e27;">1 slot only</p>
<!-- 
    <p style="color: #042e27;" id="timer"></p> -->
  </div>
  <h4 class="mb-3">Team Representative</h4>
  <form class="team" action="/registration" method="post">
    <div class="row g-3">
      <div class="col-sm-12 col-md-6 col-lg-6">
        <label for="Name" class="form-label">Full Name</label>
        <input type="text" class="form-control" name="Name" placeholder="" required autocomplete="off">
        <div class="invalid-feedback">
          Valid name is required.
        </div>
      </div>

      <div class="col-sm-12 col-md-6 col-lg-6">
        <label for="branch" class="form-label">Branch</label>
        <input type="text" class="form-control" name="branch" placeholder="" required autocomplete="off">
        <div class="invalid-feedback">
          Valid branch name is required.
        </div>
      </div>

      <div class="col-sm-12 col-12">
        <label for="teamName" class="form-label">Team Name</label>
        <div class="input-group">

          <input type="text" class="form-control" name="teamName" placeholder="" required autocomplete="off">
          <div class="invalid-feedback">
            Your team name is required.
          </div>
        </div>
      </div>

      <div class="col-sm-12">
        <label for="country" class="form-label">Current year</label>
        <select class="form-select year-select" name="year" required>
          <option value="">Choose...</option>
          <option>1st</option>
          <option>2nd</option>
        </select>
        <div class="invalid-feedback">
          Please select a valid year.
        </div>
      </div>

      <div class="col-sm-12">
        <label for="email" class="form-label">IITR Email</label>
        <div class="input-group">

          <input type="email" class="form-control" name="email" placeholder="for eg- abc@xyz.iitr.ac.in" required autocomplete="off">
          <div class="invalid-feedback">
            Your email is required.
          </div>
        </div>
      </div>

      <div class="col-sm-12">
        <label for="phone-number" class="form-label">Phone Number</label>
        <div class="input-group">
          <span class="input-group-text">+91</span>
          <input type="number" class="form-control" name="phonenumber" placeholder="" required autocomplete="off">
          <div class="invalid-feedback">
            Your phone number is required.
          </div>
        </div>
      </div>

    </div>

    <hr class="my-4">



    <hr class="my-4">



    <h4 class="mb-3">Players</h4>
    <div class="row gy-3">

      <div class="col-md-6">
        <label for="player name" class="form-label">Player-1 Name</label>
        <input type="text" class="form-control" name="name1" placeholder="" required autocomplete="off">

        <div class="invalid-feedback">
          Name is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="valo-id" class="form-label">Player-1 Valorant ID</label>
        <input type="text" class="form-control" name="valoid1" placeholder="Include #code" required autocomplete="off">
        <div class="invalid-feedback">
          Valorant ID is required
        </div>
      </div>

      <div class="col-md-6 col-sm-12">
        <label for="player name" class="form-label">Player-2 Name</label>
        <input type="text" class="form-control" name="name2" placeholder="" required autocomplete="off">

        <div class="invalid-feedback">
          Name is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="valo-id" class="form-label">Player-2 Valorant ID</label>
        <input type="text" class="form-control" name="valoid2" placeholder="" required autocomplete="off">
        <div class="invalid-feedback">
          Valorant ID is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="player name" class="form-label">Player-3 Name</label>
        <input type="text" class="form-control" name="name3" placeholder="" required autocomplete="off">

        <div class="invalid-feedback">
          Name is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="valo-id" class="form-label">Player-3 Valorant ID</label>
        <input type="text" class="form-control" name="valoid3" placeholder="" required autocomplete="off">
        <div class="invalid-feedback">
          Valorant ID is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="player name" class="form-label">Player-4 Name</label>
        <input type="text" class="form-control" name="name4" placeholder="" required autocomplete="off">

        <div class="invalid-feedback">
          Name is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="valo-id" class="form-label">Player-4 Valorant ID</label>
        <input type="text" class="form-control" name="valoid4" placeholder="" required autocomplete="off">

        <div class="invalid-feedback">
          Valorant ID is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="player name" class="form-label">Player-5 Name</label>
        <input type="text" class="form-control" name="name5" placeholder="" required autocomplete="off">

        <div class="invalid-feedback">
          Name is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="valo-id" class="form-label">Player-5 Valorant ID</label>
        <input type="text" class="form-control" name="valoid5" placeholder="" required autocomplete="off">
        <div class="invalid-feedback">
          Valorant ID is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="player name" class="form-label">Player-6 Name</label>
        <input type="text" class="form-control" name="name6" placeholder="" autocomplete="off">

        <div class="invalid-feedback">
          Name is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="valo-id" class="form-label">Player-6 Valorant ID</label>
        <input type="text" class="form-control" name="valoid6" placeholder="" autocomplete="off">

        <div class="invalid-feedback">
          Valorant ID is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="player name" class="form-label">Player-7 Name</label>
        <input type="text" class="form-control" name="name7" placeholder="" autocomplete="off">

        <div class="invalid-feedback">
          Name is required
        </div>
      </div>

      <div class="col-md-6">
        <label for="valo-id" class="form-label">Player-7 Valorant ID</label>
        <input type="text" class="form-control" name="valoid7" placeholder="" autocomplete="off">

        <div class="invalid-feedback">
          Valorant ID is required
        </div>
      </div>





    </div>

    <hr class="my-4">
    <div class="form-check container-fluid">
      <input class="form-check-input" type="checkbox" id="autoSizingCheck2" required>
      <label class="form-check-label" for="autoSizingCheck2">
        I have read the tournament rules from <a class="check-reg-href" href="/details">Details Page</a>
      </label>
    </div>

    <button class="w-100 btn btn-primary btn-lg" type="submit">Submit</button>
  </form>
</div>
</div>
<script src="/js/registration.js" charset="utf-8"></script>

</script>
<%- include("partials/footer"); -%>
